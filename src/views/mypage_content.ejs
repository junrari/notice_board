<link rel="stylesheet" href="/static/css/styles.css"/> 
<link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link rel="stylesheet" href="/static/css/styles.css"/> 
<link href="https://fonts.googleapis.com/css2?family=IBM+Plex+Sans+KR:wght@500&display=swap" rel="stylesheet">
<div class="container" >
    내가 작성한 게시글
    <table>
        <tr>
            <th>번호</th>
            <th>제목</th>
            <th>등록</th>
            <th>조회수</th>
        </tr>
           <% if(userContentlist===0) { %>
            <tr>
                <th colspan="4">
                    등록된 정보가 없습니다
                </th>
            </tr>
        <%}else{%>
                <% userContentlist.forEach(data=>{%>
                <tr>
                    <td>
                        <%= data.rn %>
                    </td>
                    <td>
                        <a href="/page/content/<%=data.num%>">
                            <%= data.title %></a>   <%if(data.image_path!==null){%>
                               <span><img src="/static/image/photo.png" class="list_photoIcon" alt="사진"></span>
                                <%}%>  
                    </td>
                    <td>
                        <%= data.pdate %>
                    </td>
                    <td>
                        <%= data.count %>
                    </td>
                </tr>
            <%})%>
        <%}%>
     
        <tr>
            <td colspan="4">
                <% if(likestart <=1){%>
                    <button disabled class="nav-button">이전</button>
                <%}else{%>
                        <button type="button"
                        onclick="location.href='/page/mypage?userConentstart=<%=userConentstart -1%>'" class="nav-button">이전</button>
                <%} for (var i=1 ; i< userConentstart; i++ ){%>
                <a href="/page/mypage?userConentstart=<%=i%>"> <%=i%> </a>
                <%}%>
                <a href="/page/mypage?userConentstart=<%=userConentstart%>" class="current-page"><%=userConentstart%></a>
                <%for (var i=userConentstart+1 ; i <= userContentpage.totalPage; i++ ){%>
                    <a href="/page/mypage?userConentstart=<%=i%>"> <%=i%> </a>
                <%} if(likestart< userContentpage.totalPage){ %>
                    <button type="button"
                                    onclick="location.href='/page/mypage?userConentstart=<%=userConentstart+1%>'" class="nav-button">다음</button>
                                <%}else{%>
                                    <button disabled class="nav-button">다음</button>
                                    <%}%>
    
                        <%=usertotalContent%> 
    
            </td>
        </tr>
    </table>
</div>